    "main": "index.js",
    "scripts": {
        "tsc": "./node_modules/typescript/bin/tsc",
        "start": "npm run tsc && node dist/server.js",
        "dev": "./node_modules/typescript/bin/tsc && node dist/server.js",
        "migration:generate": "./node_modules/typeorm/cli.js migration:generate src/database/migrations/migrations -d dist/database/datasource.js",
        "migration:run": "./node_modules/typeorm/cli.js migration:run  -d dist/database/datasource.js",
        "migration:revert": "./node_modules/typeorm/cli.js migration:revert -d dist/database/datasource.js",
        "db:seed": "node dist/database/seeders/seed.js",
        "db:init": "npm run tsc && npm run migration:run && node dist/database/seeders/seed.js",
        "db:reset": "npm run tsc && npm run migration:revert && npm run migration:run && node dist/database/seeders/seed.js",
        "test": "npm run db:reset && jest"
    },
